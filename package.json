{
  "name": "typa",
  "version": "0.3.4",
  "description": "Super-simple, zero-dependency JavaScript type checker utility.",
  "keywords": [
    "javascript",
    "checker",
    "type",
    "typeof",
    "types",
    "utility"
  ],
  "author": "Daniel Sieradski <daniel@self.agency>",
  "license": "MIT",
  "homepage": "https://gitlab.com/selfagency/typa",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/selfagency/typa.git"
  },
  "bugs": {
    "url": "https://gitlab.com/selfagency/typa/issues"
  },
  "scripts": {
    "build": "pika build",
    "test": "jest --coverage",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg",
        {
          "exclude": [
            "tests/**/*"
          ]
        }
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "pika-plugin-minify",
        {
          "ecma": 2015,
          "module": true
        }
      ],
      [
        "@pika/plugin-build-types"
      ]
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@pika/plugin-build-node": "^0.9.2",
    "@pika/plugin-build-types": "^0.9.2",
    "@pika/plugin-build-web": "^0.9.2",
    "@pika/plugin-bundle-node": "^0.9.2",
    "@pika/plugin-bundle-web": "^0.9.2",
    "@pika/plugin-standard-pkg": "^0.9.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.5.1",
    "babel-loader": "^8.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^25.5.2",
    "pika-plugin-minify": "^0.1.0",
    "prettier": "^2.0.5",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "snyk": "^1.319.1"
  },
  "snyk": true
}
